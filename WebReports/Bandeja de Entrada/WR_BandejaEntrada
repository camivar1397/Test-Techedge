    <script>
        function GetReport(idReport, option, title) {
            let divReport = document.getElementById('resultado-reporte')
            let spanTitle = document.getElementById('titulo-reporte')
            spanTitle.innerHTML = "Cargando..."
            let URL = ""
            if (idReport === [LL_REPTAG_$WR_ConsultaAdmin /]) {
                URL = "?func=ll&objId=" + idReport + "&objAction=RunReport&conHTML=0&nexturl=[LL_REPTAG_NEXTURL /]"
            } else {
                URL = "?func=ll&objId=" + idReport + "&objAction=RunReport&conHTML=0&nexturl=[LL_REPTAG_MYURL ESCAPEURL /]"
            }
            divReport.src = URL
            spanTitle.innerHTML = option + " / " + title
        }

        [LL_WEBREPORT_IF "[LL_REPTAG_$GRP_Solicitante USERINGROUP /]" == "TRUE" /] 
            GetReport([LL_REPTAG_$WR_EnvInternas /], 'Mis Comunicaciones / Enviadas', 'Internas')
        [LL_WEBREPORT_ELSEIF "[LL_REPTAG_$GRP_CGC USERINGROUP /]" == "TRUE" /]
            GetReport([LL_REPTAG_$WR_Asignaciones /], 'Mis asignaciones', 'Recibidas')
        [LL_WEBREPORT_ELSE /]
            GetReport([LL_REPTAG_$WR_RecInternas /], 'Mis Comunicaciones / Recibidas', 'Internas')
        [LL_WEBREPORT_ENDIF /]

    </script>
